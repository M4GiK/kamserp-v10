  <images/>
  <story>
    <para style="terp_default_9">[[ repeatIn(objects,'order') ]]</para>
    <para style="terp_default_9">[[ setLang(order.partner_id.lang) ]]</para>
    <blockTable colWidths="241.0,57.0,213.0" style="Table_Address_Que">
      <tr>
        <td>
          <para style="terp_default_address">
            <font color="white"> </font>
          </para>
        </td>
        <td>
          <para style="terp_default_address">
            <font color="white"> </font>
          </para>
        </td>
        <td>
          <para style="terp_default_address">[[ order.partner_id.title ]][[ order.partner_id.name ]]</para>
