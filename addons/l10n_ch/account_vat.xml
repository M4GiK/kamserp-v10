			<field name="ref_base_code_id" ref="vat_code_receivable_base_invest76"/>
			<field name="ref_tax_code_id" ref="vat_code_payable_invest"/>
			<field name="python_compute">result = round((price_unit * 0.076) / 0.05) * 0.05</field>
