        <record model="hr.action.reason">
            <field name="name">Start of shift</field>
            <field name="action_type">sign_in</field>
        </record>
        <record model="hr.action.reason">
            <field name="name">End of shift</field>
            <field name="action_type">sign_out</field>
        </record>
        <record model="hr.action.reason">
